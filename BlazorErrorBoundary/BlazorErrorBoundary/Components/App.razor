<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <base href="/"/>
    <link rel="stylesheet" href="bootstrap/bootstrap.min.css"/>
    <link rel="stylesheet" href="app.css"/>
    <link rel="stylesheet" href="BlazorErrorBoundary.styles.css"/>
    <link rel="icon" type="image/png" href="favicon.png"/>
    <HeadOutlet @rendermode="InteractiveServer"/>
</head>

<body>
<Routes @rendermode="InteractiveServer"/>
<script src="_framework/blazor.web.js"></script>
<script>
        var g_FKeyInputTimer = null;

        function OnFKeyKeyup(event)
        {
            if(g_FKeyInputTimer) clearTimeout(g_FKeyInputTimer);
            g_FKeyInputTimer = setTimeout(function()
            {
               var field = document.getElementById('test_fkey');
              var event = new Event('resolveFkey', {bubbles:true});
              field.dispatchEvent(event);

            }, 350);
        }
    </script>
</body>

</html>